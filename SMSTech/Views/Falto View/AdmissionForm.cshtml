@{
    ViewBag.Title = "AdmissionForm";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

       <!-- Form Title -->
<!-- ============================================================== -->
<div class="row page-titles">
    <div class="col-md-5 align-self-center">
        <h4 class="text-themecolor">Admission Form</h4>
   
     
    </div>
    <div class="col-md-7 align-self-center text-right">
        <div class="d-flex justify-content-end align-items-center">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                <li class="breadcrumb-item active">Admission Form</li>
            </ol>
            <button type="button" class="btn btn-info d-none d-lg-block m-l-15"><i class="fa fa-plus-circle"></i> Create New</button>
        </div>
    </div>
</div>
<!-- ============================================================== -->
<!-- End Form Title -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- Start Page Content -->
<!-- ============================================================== -->
<!-- Row -->

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                 <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist">
                    <li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#home" role="tab"><span class="hidden-sm-up"><i class="ti-home"></i></span> <span class="hidden-xs-down">Student Information</span></a> </li>
                    <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#profile" role="tab"><span class="hidden-sm-up"><i class="ti-user"></i></span> <span class="hidden-xs-down">Parent Information</span></a> </li>
                    <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#messages" role="tab"><span class="hidden-sm-up"><i class="ti-email"></i></span> <span class="hidden-xs-down">Academics Details</span></a> </li>
                </ul>
                <!-- Tab panes -->
                <div class="tab-content tabcontent-border">
                    <!--Student Tab Open-->
                    <div class="tab-pane active" id="home" role="tabpanel">
                        <div class="p-20">
                            <div class="card-header bg-info">
                                <h4 class="m-b-0 text-white">Student Admission Form</h4>
                            </div>
                            <div class="card-body">
                                <form action="StudentInformation" method="post">
                                    <div class="form-body">
                                        <h3 class="card-title">Student Information</h3>
                                        <hr>

                                        <!--Rows With 2 Taxboxes-->
                                        <div class="row p-t-20">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">GR. No</label>
                                                    <input type="text" name="GRNo" id="firstName" class="form-control" placeholder="GR. #">
                                                </div>
                                            </div>
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Roll No</label>
                                                    <input type="text" name="RollNo" id="lastName" class="form-control" placeholder="Roll No">
                                                </div>
                                            </div>
                                            <!--/span-->
                                        </div>
                                        <!--/row-->
                                        <!--Rows With 2 Combobox-->
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group ">
                                                    <label class="control-label">Class</label>
                                                </div>
                                            </div>
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group ">
                                                    <label class="control-label">Section</label>


                                                </div>
                                            </div>
                                            <!--/span-->
                                        </div>
                                        <!--/row-->

                                        <div class="row p-t-20">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">First Name</label>
                                                    <input type="text" name="StdFirstName" id="firstName" class="form-control" placeholder="First Name">
                                                </div>
                                            </div>
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Last Name</label>
                                                    <input type="text" name="StdLastName" id="lastName" class="form-control" placeholder="Last Name">
                                                </div>
                                            </div>
                                            <!--/span-->
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Gender</label>
                                                    <small class="form-control-feedback"> Select your gender </small>
                                                </div>
                                            </div>
                                            <!--/span-->
                         
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Date of Birth</label>
                                                    <input type="date" name="DOB" class="form-control" placeholder="dd/mm/yyyy">
                                                </div>
                                            </div>
                                            <!--/span-->
                                        </div>

                                        <!--Rows With 2 Combobox-->
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Religion</label>
                                                </div>
                                            </div>
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Place Of Birth</label>
                                                </div>
                                            </div>
                                            <!--/span-->
                                        </div>
                                        <!--/row-->
                                        <!--Rows With 2 Combobox-->
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Blood Group</label>
                                                </div>
                                            </div>
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Student House</label>
                                                    @*@Html.TextBoxFor(x => x.StudentHouse, new { @placeholder = "", @class = "form-control", @required = "required", @type = "text" })*@




                                                    </div>
                                            </div>
                                            <!--/span-->
                                        </div>
                                        <!--/row-->

                                        <h3 class="box-title m-t-40">Address</h3>
                                        <hr>
                                        <div class="row">
                                            <div class="col-md-12 ">
                                                <div class="form-group">
                                                    <label>Street</label>
                                                    <input type="text" class="form-control" name="Street">
                                                </div>
                                            </div>
                                        </div>
                                        <!--Rows With 2 Combobox-->
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">Area</label>
                                                    <input type="text" class="form-control" name="Area">

                                                   
                                                </div>
                                            </div>
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="control-label">City</label>
                                                    <input type="text" class="form-control" name="City">
 
                                                </div>
                                            </div>
                                            <!--/span-->
                                        </div>
                                        <!--/row-->
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Post Code</label>
                                                    <input type="text" class="form-control" name="PCode">
                                                </div>
                                            </div>
                                            <!--/span-->
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Country</label>
                                                    <input type="text" class="form-control" name="Country">
                                                   
                                                </div>
                                            </div>
                                            <!--/span-->
                                        </div>
                                    </div>

                                    <div class="form-actions">
                                        <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i>Save</button>
                                        <button type="button" class="btn btn-inverse">Cancel</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                    <!--Student Tab Close-->
                    <!--Parent Tab Open-->
                        
                            <div class="tab-pane  p-20" id="profile" role="tabpanel">
                                <!--Father Details-->

                                <div class="card-header bg-info">
                                    <h4 class="m-b-0 text-white">Father Details</h4>
                                </div>
                                <form action="ParentDetails" method="post">
                                    <!--Rows With 2 Taxboxes-->
                                    <div class="row p-t-20">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label">Father Name</label>
                                                <input type="text" name="FFirstName" id="firstName" class="form-control" placeholder="Father Name">
                                            </div>
                                        </div>
                                        <!--/span-->
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label">CNIC</label>
                                                <input type="text" name="FCNIC" id="lastName" class="form-control" placeholder="CNIC No">
                                            </div>
                                        </div>
                                        <!--/span-->
                                    </div>
                                    <!--/row-->
                                    <!--Rows With 2 Taxboxes-->
                                    <div class="row p-t-20">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label">Contact No</label>
                                                <input type="text" name="Fcontactno" id="firstName" class="form-control" placeholder="Contact No">
                                            </div>
                                        </div>
                                        <!--/span-->
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label">Occupation</label>
                                                <input type="text" name="FOccupation" id="firstName" class="form-control" placeholder="">
                                            </div>
                                        </div>
                                        <!--/span-->
                                    </div>
                                    <!--/row-->
                                    <!--Mother Details-->

                                    <div class="card-header bg-info">
                                        <h4 class="m-b-0 text-white">Mother Details</h4>
                                    </div>

                                    <!--Rows With 2 Taxboxes-->
                                    <div class="row p-t-20">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label">Mother Name</label>
                                                <input type="text" name="MFirstName" id="firstName" class="form-control" placeholder="Mother Name">
                                            </div>
                                        </div>
                                        <!--/span-->
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label">CNIC</label>
                                                <input type="text" name="MCNIC" id="lastName" class="form-control" placeholder="CNIC No">
                                            </div>
                                        </div>
                                        <!--/span-->
                                    </div>
                                    <!--/row-->
                                    <!--Rows With 2 Taxboxes-->
                                    <div class="row p-t-20">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label">Contact No</label>
                                                <input type="text" name="MContactNo" id="firstName" class="form-control" placeholder="Contact No">
                                            </div>
                                        </div>
                                        <!--/span-->
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="control-label">Occupation</label>
                                                <input type="text" name="MOccupation" id="firstName" class="form-control" placeholder="">

                                            </div>
                                        </div>
                                        <!--/span-->
                                    </div>
                                    <!--/row-->
                                   
                                    <div class="form-actions">
                                        <button type="submit" class="btn btn-success img-responsive model_img" id="sa-success"> <i class="fa fa-check"></i>Save</button>
                                       
                                        <button type="button" class="btn btn-inverse">Cancel</button>
                                    </div>
                                </form>
                            </div>

                 
                            <!--parent Tab Close-->
                            <!--Academics Tab Open-->

                            <div class="tab-pane p-20" id="messages" role="tabpanel">
                                <div class="card-header bg-info">
                                    <h4 class="m-b-0 text-white">Previous School Information</h4>
                                </div>
                                <div class="card-body">
                                    <form action="#">
                                        <div class="form-body">
                                            <!--Rows With 1 Text 1 Combo-->
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Previous School Name</label>
                                                        <input type="text" id="firstName" class="form-control" placeholder="Previous School">
                                                    </div>
                                                </div>
                                                <!--/span-->
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Previous Class</label>
                                                        <select class="form-control custom-select">

                                                            
                                                              <option>Pakistan</option>                                                        
                                                            <option>India</option>
                                                            
                                                               
                                                        </select>
                                                    </div>
                                                </div>
                                                <!--/span-->
                                            </div>
                                            <!--/row-->
                                           <!--Rows With 1 Taxboxes-->
                                            <div class="row p-t-20">
                                                <div class="col-md-12">
                                                    <div class="form-group">
                                                        <label class="control-label">Previous School Address</label>
                                                        <input type="text" id="firstName" class="form-control" placeholder="Previous School Address">
                                                    </div>
                                                </div>
                                                <!--/span-->
                                            </div>
                                            <!--/row-->
                                          
                                            <!--/row-->
                                       
                                        </div>

                                        <div class="form-actions">
                                            <button type="submit" class="btn btn-success"> <i class="fa fa-check"></i>Save</button>
                                            <button type="button" class="btn btn-inverse">Cancel</button>
                                        </div>
                                    </form>
                                </div>


                            </div>

                            <!--Academics Tab Close-->

                    </div>
                </div>
        </div>
    </div>
   
</div>
<!-- ============================================================== -->
<!-- End PAge Content -->
<!-- ============================================================== -->
